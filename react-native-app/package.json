{
  "name": "foodprint",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "a": "npm run android",
    "android": "react-native run-android",
    "a:codepush": "appcenter codepush release-react -a Ben-xD/Foodprint -d prod",
    "android:local": "ENVFILE=.env.android.local react-native run-android",
    "a:release": "react-native run-android --variant=release",
    "a:bundle": "cd android && ./gradlew bundleRelease",
    "a:apk": "cd android && ./gradlew assembleRelease",
    "ios:local": "ENVFILE=.env.ios.local react-native run-ios --simulator",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release --device",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest  --coverage",
    "test:debug": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "a:devmenu": "adb shell input keyevent 82",
    "reset-cache": "react-native start --reset-cache",
    "android:deploy": "cd android && fastlane supply --aab app/build/outputs/bundle/release/app-release.aab"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "@gorhom/paper-onboarding": "^0.3.1",
    "@invertase/react-native-apple-authentication": "^1.0.0",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/google-signin": "^3.0.4",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/netinfo": "^5.5.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-native-firebase/auth": "^6.2.0",
    "@react-navigation/bottom-tabs": "^5.2.4",
    "@react-navigation/native": "^5.1.3",
    "@react-navigation/stack": "^5.2.6",
    "apollo-boost": "^0.4.7",
    "appcenter": "3.0.1",
    "appcenter-analytics": "3.0.1",
    "appcenter-crashes": "3.0.1",
    "graphql": "^14.6.0",
    "jest-when": "^2.7.1",
    "lottie-react-native": "^3.3.2",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "16.9.0",
    "react-apollo": "^3.1.3",
    "react-native": "0.62.3",
    "react-native-camera": "^3.19.0",
    "react-native-code-push": "^6.2.0",
    "react-native-config": "^0.12.0",
    "react-native-elements": "^1.2.7",
    "react-native-floating-action": "^1.20.0",
    "react-native-geolocation-service": "^3.1.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.2.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-responsive-screen": "^1.4.0",
    "react-native-safe-area-context": "^0.6.4",
    "react-native-screens": "^2.4.0",
    "react-native-snackbar": "^2.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^11.0.1",
    "react-native-vector-icons": "^6.6.0",
    "victory-native": "^34.1.0",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@testing-library/react-native": "^5.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "^16.12.0",
    "snyk": "^1.297.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  }
}
